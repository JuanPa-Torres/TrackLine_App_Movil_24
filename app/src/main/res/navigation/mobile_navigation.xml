<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.trackline24.ui.home.HomeFragment"
        android:label="INICIO"
        tools:layout="@layout/fragment_home"
        app:launchSingleTop="true"/>

    <fragment
        android:id="@+id/navigation_route"
        android:name="com.example.trackline24.PlanesEntrenamientoFragment"
        android:label="PLANES"
        tools:layout="@layout/fragment_route" />

    <fragment
        android:id="@+id/navigation_news"
        android:name="com.example.trackline24.EventosFragment"
        android:label="EVENTOS"
        tools:layout="@layout/fragment_eventos" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.trackline24.ui.profile.ProfileFragment"
        android:label="PERFIL"
        tools:layout="@layout/fragment_profile" />

</navigation>